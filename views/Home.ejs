<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Node.js Blog</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    <style>
        body {
            padding-top: 20px;
            padding-bottom: 20px;
        }
        .jumbotron {
          margin-top: 10px;
        }
    </style>
</head>
<body>
    <div class="container">
        <%- include('partials/navbar') %>
        <div class="jumbotron">
            <h1>Welcome Here !</h1>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="list-group">
                  <!-- loop over blog posts and render them -->
                  LIST_OF_USERS
                  <br />
                  <% for (var i = 0; i < user.length; i++) { %>
                    <h2>
                        <%= user[i].dataValues.name %>
                    </h2>
                  <% } %>
                </div>
            </div>
        </div>
    </div>
</body>
</html>